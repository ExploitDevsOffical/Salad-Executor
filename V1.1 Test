		end)
	
		TextButton_6.MouseButton1Click:Connect(function()
			loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua'))()
		end)
	
		TextButton_3.MouseButton1Click:Connect(function()
			print("Successfuly Injected! - Made by Exploit Devs Offical")
		end)
	
		MinusButton.MouseButton1Click:Connect(function()
			MainFrame.Visible = false
		end)
		local UserInputService = game:GetService("UserInputService")
	
		UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
			if not gameProcessedEvent then
				if input.KeyCode == Enum.KeyCode.LeftControl then
					MainFrame.Visible = true
				end
			end
		end)
	
		XButton.MouseButton1Click:Connect(function()
			-- Replace this with your desired functionality for closing the UI
			ScreenGui:Destroy() -- Destroys the entire ScreenGui, effectively closing the UI
		end)
	end)
	
end
coroutine.wrap(JHZNAB_fake_script)()
local function QTFLWK_fake_script() -- GetUsername.LocalScript 
	local script = Instance.new('LocalScript', GetUsername)

	local player = game.Players.LocalPlayer
	
	if player then
		script.Parent.Text = player.Name
	end
end
coroutine.wrap(QTFLWK_fake_script)()
local function YUJRD_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local ScreenGui = script.Parent
	local TextButton = ScreenGui.MainFrame.TextButton
	local Frame = ScreenGui.MainFrame
	
	TextButton.MouseButton1Up:Connect(function()
		Frame.Visible = false
	end)
	
end
coroutine.wrap(YUJRD_fake_script)()
